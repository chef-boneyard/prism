{
  "version": "2",
  "build_cookbook": {
    "name": "build_cookbook",
    "path": ".delivery/build_cookbook"
  },
  "skip_phases": [
    "deploy",
    "security",
    "quality",
    "functional"
  ],
  "delivery-truck": {
    "publish": {
      "github": "chef/prism"
    }
  },
  "job_dispatch": {
    "version": "v2",
    "filters": {
      "default": {
        "platform": ["ubuntu"],
        "platform_version" : ["16.04"]
      }
    }
  },
  "dependencies": []
}
